    <div class="timeBlock">
        <span class="time"></span>
    </div>
    <div class="topMenu">
        <div class="leftMenu">
            <div class="phones">
                
                <span class="phones-toggle" onclick="openDelivery()"><span class="pc">Контакты</span><img src="../icons/tel.svg"><span class="arrow">▼</span></span>
                <div id="delivery-popup" class="delivery-list-popup">
                    <div class="contacts">
                    <div class="title">Служба доставки:</div>
                            <ul>
                          <li>
                                <a href="tel:+375(44)518-17-76" class="t44">(44) 518-17-76</a>
                            </li>
                          <li>
                                <a href="tel:+375(33)175-17-54" class="t33">(33) 175-17-54</a>
                            </li>
